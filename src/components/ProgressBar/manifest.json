{
  "displayName": "Progress Bar",
  "defaultWidth": 375,
  "defaultHeight": 4,
  "components": "./index.js",
  "icon": "./icon.png",
  "resizeY": true,
  "props": [
    {
      "name": "currentValue",
      "displayName": "Progress Value",
      "type": "number",
      "default": "10"
    },
    {
      "name": "maxValue",
      "displayName": "Maximum Value",
      "type": "number",
      "default": "100"
    },
    {
      "name": "color",
      "displayName": "Color",
      "type": "color",
      "default": "@primary"
    },
    {
      "name": "backgroundColor",
      "displayName": "Background Color",
      "type": "color",
      "default": "@primaryLight"
    }
  ]
}